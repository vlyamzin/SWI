  },
  "homepage": "https://github.com/vlyamzin/SWI#readme",
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.8",
    "@types/ws": "0.0.39",
    "debug": "2.6.3",
    "express": "4.15.2",
    "gulp": "3.9.1",
    "gulp-typescript": "3.1.5",
    "node-static": "^0.7.9",
    "nodemon": "1.11.0",
    "pre-commit": "1.2.2",
    "run-sequence": "1.2.2",
    "source-map-loader": "0.2.0",
